#######################################
# On deployment, rename this file to '.htaccess'
#######################################

# Force HTTPS - without it, Chrome won't allow location services.
RewriteEngine On 
RewriteCond %{SERVER_PORT} 80 
RewriteCond %{REQUEST_URI} geo-poetry-demo 
RewriteRule ^(.*)$ https://jordan-rickman.net/geo-poetry-demo/$1 [R,L]